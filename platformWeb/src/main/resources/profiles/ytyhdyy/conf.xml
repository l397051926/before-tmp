    <!--MemCached配置-->
    <bean id="MemCachedConf" class="com.gennlife.platform.configuration.MemCachedConf">
        <property name="servers">
            <array value-type="java.lang.String">
                <value>192.168.0.95:11211</value>
            </array>
        </property>
        <property name="weights">
            <array value-type="java.lang.Integer">
                <value>2</value>
            </array>
        </property>
        <property name="initConnections" value="5"/>
        <property name="minConnections" value="5" />
        <property name="maxConnections" value="250" />
        <property name="maxIdle" value="21600000" />
        <property name="maintSleep" value="30" />
        <property name="setNagle" value="false" />
        <property name="socketConnectTO" value="0" />
        <property name="socketTO" value="3000" />
